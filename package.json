            "transitionTimingFunction": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])ease(|-)[^`'\"\\s]*",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(141, 179, 184, 1)"
              }
            },
            "transitionDelay": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])delay-[^`'\"\\s]*",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(141, 179, 184, 1)"
              }
            },
            "animation": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])animate-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(255, 176, 202, 1)"
              }
            },
            "transform": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])transform(|-)[^`'\"\\s]*",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(255, 176, 202, 1)"
              }
            },
            "transformOrigin": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])origin-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(255, 176, 202, 1)"
              }
            },
            "scale": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])scale-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(255, 176, 202, 1)"
              }
            },
            "rotate": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])(|-)rotate-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(255, 176, 202, 1)"
              }
            },
            "translate": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])(|-)translate-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(255, 176, 202, 1)"
              }
            },
            "skew": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])(|-)skew-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(255, 176, 202, 1)"
              }
            },
            "text": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])text-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(171, 169, 169, 1)"
              }
            },
            "textDecoration": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])(underline|line-through|no-underline)",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(171, 169, 169, 1)"
              }
            },
            "textTransform": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])(uppercase|lowercase|capitalize|normal-case)",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(171, 169, 169, 1)"
              }
            },
            "textOverflow": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])(truncate|overflow-ellipsis|overflow-clip)",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(171, 169, 169, 1)"
              }
            },
            "verticalAlign": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])align-[^`'\"\\s]+",
              "options": {
