            "table": {
              "enable": false,
              "regex": "(?<=[:`'\"\\s])(table-fixed|table-auto|table)",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(255, 210, 105, 1)"
              }
            },
            "borderCollapse": {
              "enable": false,
              "regex": "(?<=[:`'\"\\s])border-(collapse|separate)",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(255, 210, 105, 1)"
              }
            },
            "transition": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])transition(|-)[^`'\"\\s]*",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(141, 179, 184, 1)"
              }
            },
            "transitionDuration": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])duration-[^`'\"\\s]*",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(141, 179, 184, 1)"
              }
