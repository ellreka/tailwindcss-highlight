{
  "name": "tailwindcss-highlight",
  "version": "1.2.0",
  "main": "out/extension.js",
  "license": "MIT",
  "publisher": "ellreka",
  "repository": {
    "type": "git",
    "url": "https://github.com/ellreka/tailwindcss-highlight.git"
  },
  "keywords": [
    "tailwindcss"
  ],
  "icon": "assets/icon.png",
  "engines": {
    "vscode": "^1.56.0"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "title": "tailwindcss highlight",
      "properties": {
        "tailwindcss-highlight.languages": {
          "default": [
            "html",
            "javascript",
            "javascriptreact",
            "typescript",
            "typescriptreact"
          ],
          "title": "language",
          "type": "array"
        },
        "tailwindcss-highlight.configs": {
          "default": {
            "padding": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])(|-)p(|t|b|r|l|x|y)-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(187, 196, 136, 1)"
              }
            },
            "margin": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])(|-)m(|t|b|r|l|x|y)-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(173, 134, 91, 1)"
              }
            },
            "space": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])(|-)space-(x|y)-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(173, 134, 91, 1)"
              }
            },
            "border": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])border(?!-collapse|-separate)(|-)[^`'\"\\s]*",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(222, 195, 138, 1)"
              }
            },
            "rounded": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])rounded(|-)[^`'\"\\s]*",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(222, 195, 138, 1)"
              }
            },
            "divide": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])divide(|-)[^`'\"\\s]*",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(222, 195, 138, 1)"
              }
            },
            "ring": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])ring(|-)[^`'\"\\s]*",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(222, 195, 138, 1)"
              }
            },
            "width": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])(|-)(|max|min)(|-)w-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(160, 202, 222, 1)"
              }
            },
            "height": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])(|-)(|max|min)(|-)h-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(230, 177, 161, 1)"
              }
            },
            "flex": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])(|inline-)flex(|-)[^`'\"\\s]*",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(192, 175, 224, 1)"
              }
            },
            "grid": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])grid(|-)[^`'\"\\s]*",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(192, 175, 224, 1)"
              }
            },
            "order": {
              "enable": false,
              "regex": "(?<=[:`'\"\\s])(|-)order-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(192, 175, 224, 1)"
              }
            },
            "gridColumn": {
              "enable": false,
              "regex": "(?<=[:`'\"\\s])col(|-)[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(192, 175, 224, 1)"
              }
            },
            "gridRow": {
              "enable": false,
              "regex": "(?<=[:`'\"\\s])row(|-)[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(192, 175, 224, 1)"
              }
            },
            "gridAutoColumns": {
              "enable": false,
              "regex": "(?<=[:`'\"\\s])auto-cols-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(192, 175, 224, 1)"
              }
            },
            "gridAutoRows": {
              "enable": false,
              "regex": "(?<=[:`'\"\\s])auto-rows-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(192, 175, 224, 1)"
              }
            },
            "gap": {
              "enable": false,
              "regex": "(?<=[:`'\"\\s])gap-[^`'\"\\s]+",
              "options": {
                "color": "black",
