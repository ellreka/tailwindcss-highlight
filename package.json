                "backgroundColor": "rgba(171, 169, 169, 1)"
              }
            },
            "whitespace": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])whitespace-(normal|nowrap|pre(-line|-wrap|))",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(171, 169, 169, 1)"
              }
            },
            "wordBreak": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])break-(normal|words|all)",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(171, 169, 169, 1)"
              }
            },
            "font": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])font-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(171, 169, 169, 1)"
              }
            },
            "fontStyle": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])(italic|not-italic)",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(171, 169, 169, 1)"
              }
            },
            "leading": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])leading-[^`'\"\\s]+",
              "options": {
                "enable": true,
                "color": "black",
                "backgroundColor": "rgba(171, 169, 169, 1)"
              }
            },
            "tracking": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])tracking-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(171, 169, 169, 1)"
              }
            },
            "background": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])bg-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "rgba(117, 224, 165, 1)"
              }
            },
            "gradientColorStops": {
              "enable": true,
              "regex": "(?<=[:`'\"\\s])(from|via|to)-[^`'\"\\s]+",
              "options": {
